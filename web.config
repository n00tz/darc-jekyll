<configuration>
    <system.webServer>
        <rewrite>
            <rules>
                <rule name="daltonhamfest" stopProcessing="true">
                    <match url="(.*)" />
                    <conditions logicalGrouping="MatchAny">
                        <add input="{HTTP_HOST}" pattern="^daltonhamfest\.com$" />
                        <add input="{HTTP_HOST}" pattern="^www\.daltonhamfest\.com$" />
                        <add input="{HTTP_HOST}" pattern="^daltonhamfest\.net$" />
                        <add input="{HTTP_HOST}" pattern="^www\.daltonhamfest\.net$" />
                    </conditions>
                    <action type="Redirect" url="http://www.w4drc.com/hamfest/{R:0}" appendQueryString="false" redirectType="Permanent" />
                </rule>
            </rules>
        </rewrite>
    </system.webServer>
</configuration>